<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.NumberMapper">
    <select id="selectNumberById" parameterType="long" resultType="String">
        <!-- SELECT number
        FROM number_table
        WHERE id = #{id} -->
        
        <!-- SELECT USER_NAME FROM T_USER_INFO WHERE USER_NAME='lwhathamrr' -->
        select 
			USER_NAME,
			USER_ID,
			USER_PW,
			USER_EMAIL
		from t_user_info
		LIMIT 0,10
        
    </select>
</mapper>
